{
	"name": "oliverthiele/ot-flippingbook",
	"type": "typo3-cms-extension",
	"description": "TYPOP3 content element for the integration of documents created with the FlippingBook Publisher (https://flippingbook.com/digital-publishing-software)",
	"homepage": "https://www.oliver-thiele.de/",
	"authors": [
		{
			"name": "Oliver Thiele",
			"email": "mail@oliver-thiele.de",
			"role": "Developer",
			"homepage": "https://www.oliver-thiele.de/"
		}
	],
	"license": [
		"GPL-2.0-or-later"
	],
	"keywords": [
		"TYPO3 CMS"
	],
	"require": {
		"typo3/cms-core": "^13.4"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest"
	},
	"autoload": {
		"psr-4": {
			"OliverThiele\\OtFlippingbook\\": "Classes/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "ot_flippingbook"
		}
	}
}
